<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot_swagger2.mapper.UserMapper">

    <select id="selectUserByAccountAndPassword" resultType="com.example.springboot_swagger2.model.User">
        select * from token.user where account=#{account} and password=#{password}
    </select>
</mapper>